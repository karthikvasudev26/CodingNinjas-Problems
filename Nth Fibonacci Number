#include<bits/stdc++.h>
using namespace std;
int main()
{
        /*
        *  Write your code here. 
         *  Read input as specified in the question.
         *  Print output as specified in the question.
        */
        int n;
        cin>>n;
        int a=1,b=1;
        for(int i=1;i<n;i++)
        {
              int nextNum=a+b;
              a=b;
              b=nextNum;
        }
        cout<<a;
}
